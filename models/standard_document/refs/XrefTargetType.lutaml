class XrefTargetType {
  definition {
     Listing of multiple cross-reference targets to be combined under the one element
  }
  +target: IdElement {
    definition {
      Cross-reference target to be included in the listing.
    }
  }
  +connective: XrefConnectiveType {
    definition {
      Connective linking this target to its predecessor. _from/to_ are presumed to nest more closely than _and_ or _or_.
    }
  }
}
